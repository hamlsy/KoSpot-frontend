{
  "name": "KoSpot",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "serve:dev": "node scripts/setup-env.js development setup && vue-cli-service serve",
    "build": "vue-cli-service build",
    "build:dev": "node scripts/setup-env.js development setup && vue-cli-service build --mode development",
    "build:prod": "node scripts/setup-env.js production setup && vue-cli-service build --mode production",
    "lint": "vue-cli-service lint",
    "fix-imports": "node scripts/fix-imports.js",
    "dedup": "node scripts/deduplicator.js",
    "restore": "node scripts/restore-imports.js",
    "optimize": "npm run fix-imports && npm run dedup",
    "env:setup": "node scripts/setup-env.js",
    "env:validate": "node scripts/setup-env.js development validate",
    "env:show": "node scripts/setup-env.js development show"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^6.7.1",
    "@stomp/stompjs": "^7.1.1",
    "axios": "^1.7.9",
    "chart.js": "^4.4.9",
    "core-js": "^3.8.3",
    "crypto-js": "^4.2.0",
    "pinia": "^2.1.7",
    "sockjs-client": "^1.6.1",
    "vue": "^3.5.17",
    "vue-countup-v3": "^1.4.2",
    "vue-router": "^4.5.1",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@babel/core": "^7.24.0",
    "@babel/eslint-parser": "^7.24.0",
    "@storybook/vue3": "^9.0.12",
    "@vue/cli-plugin-babel": "^5.0.8",
    "@vue/cli-plugin-eslint": "^5.0.8",
    "@vue/cli-plugin-router": "^5.0.8",
    "@vue/cli-service": "^5.0.8",
    "@vue/compiler-sfc": "^3.5.14",
    "autoprefixer": "^10.4.16",
    "chalk": "^4.1.2",
    "eslint": "^7.32.0",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-vue": "^9.19.2",
    "glob": "^11.0.3",
    "postcss": "^8.4.38",
    "sass": "^1.69.5"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/vue3-essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "@babel/eslint-parser",
      "ecmaVersion": 2021,
      "sourceType": "module",
      "requireConfigFile": false,
      "babelOptions": {
        "configFile": "./babel.config.js"
      }
    },
    "rules": {
      "no-unused-vars": "off",
      "no-mixed-spaces-and-tabs": 0,
      "vue/multi-word-component-names": 0
    },
    "globals": {
      "kakao": "readonly",
      "defineProps": "readonly",
      "defineEmits": "readonly",
      "defineExpose": "readonly",
      "withDefaults": "readonly"
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
}
